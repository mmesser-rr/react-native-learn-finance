{
  "name": "u3infotech",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "release": "react-native run-ios --configuration Release",
    "ios8": "react-native run-ios --simulator='iPhone 8'",
    "bundle": "react-native bundle --entry-file index.js --platform ios --dev false --reset-cache --bundle-output unused.jsbundle --sourcemap-output index.ios.map",
