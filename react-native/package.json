  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [],
    "testPathIgnorePatterns": [
      "/amplify/",
      "/node_modules/"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/build"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "node"
    ]
  },
  "lint-staged": {
    "*.tsx": "eslint",
    "*.ts": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  }
